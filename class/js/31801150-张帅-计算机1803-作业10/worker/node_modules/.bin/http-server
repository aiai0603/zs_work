#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../_http-server@0.12.3@http-server/bin/http-server" "$@"
  ret=$?
else 
  node  "$basedir/../_http-server@0.12.3@http-server/bin/http-server" "$@"
  ret=$?
fi
exit $ret
